<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta name="description" content="" />
        <meta name="keywords" content="" />
        <meta name="author" content="OVONO BENG YVON YVES NOEL" />

        <!-- Favicon icon-->
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset ('assets/images/brand/logo.png')}}" />

        <!-- darkmode js -->
        <script src="{{ asset ('assetsIndex/js/vendors/darkMode.js') }}') }}"></script>

        <!-- Libs CSS -->
        <link href="{{ asset ('assetsIndex/fonts/feather/feather.css') }}" rel="stylesheet" />
        <link href="{{ asset ('assetsIndex/libs/bootstrap-icons/font/bootstrap-icons.min.css') }}" rel="stylesheet" />
        <link href="{{ asset ('assetsIndex/libs/simplebar/dist/simplebar.min.css') }}" rel="stylesheet" />

        <!-- Theme CSS -->
        <link rel="stylesheet" href="{{ asset ('assetsIndex/css/theme.min.css') }}" />

        <link rel="canonical" href="https://geeksui.codescandy.com/geeks/index.html" />
        <link href="{{ asset ('assetsIndex/libs/tiny-slider/dist/tiny-slider.css') }}" rel="stylesheet" />
        <title>{% block title %}Bienvenue{% endblock %}</title>
    </head>

    <body>
        <main>
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg">
                <div class="container-fluid px-0">
                    <a class="navbar-brand" href="{{ path ('accueil') }}"><img src="{{ asset ('assetsIndex/images/brand/logo/logo-banniere.png') }}" height="40" width="140" alt="Geeks" /></a>
                    <!-- Mobile view nav wrap -->
                    <div class="ms-auto d-flex align-items-center order-lg-3">
                        <div class="dropdown">
                            <button
                                class="btn btn-light btn-icon rounded-circle d-flex align-items-center"
                                type="button"
                                aria-expanded="false"
                                data-bs-toggle="dropdown"
                                aria-label="Toggle theme (auto)">
                                <i class="bi theme-icon-active"></i>
                                <span class="visually-hidden bs-theme-text">Toggle theme</span>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-end shadow" aria-labelledby="bs-theme-text">
                                <li>
                                    <button type="button" class="dropdown-item d-flex align-items-center" data-bs-theme-value="light" aria-pressed="false">
                                        <i class="bi theme-icon bi-sun-fill"></i>
                                        <span class="ms-2">Light</span>
                                    </button>
                                </li>
                                <li>
                                    <button type="button" class="dropdown-item d-flex align-items-center" data-bs-theme-value="dark" aria-pressed="false">
                                        <i class="bi theme-icon bi-moon-stars-fill"></i>
                                        <span class="ms-2">Dark</span>
                                    </button>
                                </li>
                                <li>
                                    <button type="button" class="dropdown-item d-flex align-items-center active" data-bs-theme-value="auto" aria-pressed="true">
                                        <i class="bi theme-icon bi-circle-half"></i>
                                        <span class="ms-2">Auto</span>
                                    </button>
                                </li>
                            </ul>
                        </div>
                        <ul class="navbar-nav navbar-right-wrap ms-2 flex-row d-none d-md-block">
                            
                            <li class="dropdown ms-2 d-inline-block position-static">
                                <a class="rounded-circle" href="#" data-bs-toggle="dropdown" data-bs-display="static" aria-expanded="false">
                                    <div class="avatar avatar-md avatar-indicators avatar-online">
                                        <img alt="avatar" src="{{ asset ('images/photoUsers/user.png') }}" class="rounded-circle" />
                                    </div>
                                </a>
                                <div class="dropdown-menu dropdown-menu-end position-absolute mx-3 my-5">
                                    <div class="dropdown-item">
                                        <div class="d-flex">
                                            <div class="avatar avatar-md avatar-indicators avatar-online">
                                                <img alt="avatar" src="{{ asset ('images/photoUsers/user.png') }}" class="rounded-circle" />
                                            </div>
                                            <div class="ms-3 lh-1">
                                                <h5 class="mb-1">Anonyme</h5>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="dropdown-divider"></div>
                                    <ul class="list-unstyled">
                                        <li class="dropdown-submenu dropstart-lg">
                                            <a class="dropdown-item dropdown-list-group-item dropdown-toggle" href="#">
                                                <i class="fe fe-circle me-2"></i>
                                                Status
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    <a class="dropdown-item" href="#">
                                                        <span class="badge-dot bg-success me-2"></span>
                                                        Online
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="dropdown-item" href="#">
                                                        <span class="badge-dot bg-secondary me-2"></span>
                                                        Offline
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="dropdown-item" href="#">
                                                        <span class="badge-dot bg-warning me-2"></span>
                                                        Away
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="dropdown-item" href="#">
                                                        <span class="badge-dot bg-danger me-2"></span>
                                                        Busy
                                                    </a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="{{ path ('app_register')}}">
                                                <i class="fe fe-user-plus me-2"></i>
                                                {% trans %}Créer un compte{% endtrans %}
                                            </a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="{{ path ('app_login') }}">
                                                <i class="fe fe-unlock me-2"></i>
                                                {% trans %}Se connecter{% endtrans %}
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                        </ul>
                    </div>
                    <div>
                        <!-- Button -->
                        <button
                            class="navbar-toggler collapsed ms-2"
                            type="button"
                            data-bs-toggle="collapse"
                            data-bs-target="#navbar-default"
                            aria-controls="navbar-default"
                            aria-expanded="false"
                            aria-label="Toggle navigation">
                            <span class="icon-bar top-bar mt-0"></span>
                            <span class="icon-bar middle-bar"></span>
                            <span class="icon-bar bottom-bar"></span>
                        </button>
                    </div>
                    <!-- Collapse -->
                    <div class="collapse navbar-collapse" id="navbar-default">
                        <ul class="navbar-nav mt-3 mt-lg-0">
                            <li class="nav-item dropdown">
                                <a class="nav-link" href="{{ path ('accueil') }}" id="navbarBrowse" >
                                {% trans %}Accueil{% endtrans %}
                                </a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link" href="{{ path ('quick_pay') }}" id="navbarLanding" >
                                QuickPay</a>
                            </li>

							<li class="nav-item dropdown">
                                <a class="nav-link" href="{{ path ('securite')}}" id="navbarAccount">
                                {% trans %}Sécurité{% endtrans %}</a>
                            </li>

                            <li class="nav-item dropdown">
                                <a class="nav-link" href="#" id="navbarAccount" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{% trans %}Agents{% endtrans %}</a>
                            </li>

							<li class="nav-item dropdown">
                                <a class="nav-link" href="{{ path ('contact')}}" id="navbarAccount">
                                {% trans %}Contact{% endtrans %}</a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fe fe-more-horizontal"></i>
                                </a>
                                <div class="dropdown-menu dropdown-menu-md" aria-labelledby="navbarDropdown">
                                    <div class="list-group">
                                        <a class="list-group-item list-group-item-action border-0" href="{{ path ('politiqueDeConfidentialite')}}">
                                            <div class="d-flex align-items-center">
                                                <i class="fe fe-file-text fs-3 text-primary"></i>
                                                <div class="ms-3">
                                                    <h5 class="mb-0">Notre politique de confidentialité</h5>
                                                    <p class="mb-0 fs-6">Browse the all documentation</p>
                                                </div>
                                            </div>
                                        </a>
                                        <a class="list-group-item list-group-item-action border-0" href="{{ path ('termesEtConditions')}}">
                                            <div class="d-flex align-items-center">
                                                <i class="bi bi-files fs-3 text-primary"></i>
                                                <div class="ms-3">
                                                    <h5 class="mb-0">Termes et conditions</h5>
                                                    <p class="mb-0 fs-6">Bunch of Snippet</p>
                                                </div>
                                            </div>
                                        </a>
                                        <a class="list-group-item list-group-item-action border-0" href="{{ path ('supportEtAide') }}">
                                            <div class="d-flex align-items-center">
                                                <i class="fe fe-layers fs-3 text-primary"></i>
                                                <div class="ms-3">
                                                    <h5 class="mb-0">
                                                        Support et aide
                                                        <span class="text-primary ms-1" id="changelog"></span>
                                                    </h5>
                                                    <p class="mb-0 fs-6">See what's new</p>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>

			{% block stylesheets %}
					{# {{ encore_entry_link_tags('app') }} #}
			{% endblock %}

			{% block body %}{% endblock %}

			{% block javascripts %}
				{# {{ encore_entry_script_tags('app') }}
				{% block importmap %}{{ importmap('app') }}{% endblock %} #}
			{% endblock %}
		</main>

	</main>
        <!-- Footer -->
        {% include "includes/_footer.html.twig" %}

        <!-- Scroll top -->
        <div class="btn-scroll-top">
            <svg class="progress-square svg-content" width="100%" height="100%" viewBox="0 0 40 40">
                <path d="M8 1H32C35.866 1 39 4.13401 39 8V32C39 35.866 35.866 39 32 39H8C4.13401 39 1 35.866 1 32V8C1 4.13401 4.13401 1 8 1Z" />
            </svg>
        </div>

        <!-- Scripts -->
        <!-- Libs JS -->
        <script src="{{ asset ('assetsIndex/libs/@popperjs/core/dist/umd/popper.min.js') }}"></script>
        <script src="{{ asset ('assetsIndex/libs/bootstrap/dist/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset ('assetsIndex/libs/simplebar/dist/simplebar.min.js') }}"></script>

        <!-- Theme JS -->
        <script src="{{ asset ('assetsIndex/js/theme.min.js') }}"></script>

        <script src="{{ asset ('assetsIndex/libs/tiny-slider/dist/min/tiny-slider.js') }}"></script>

        <script src="{{ asset ('assetsIndex/libs/tippy.js') }}/dist/tippy-bundle.umd.min.js') }}"></script>
        <script src="{{ asset ('assetsIndex/js/vendors/tnsSlider.js') }}"></script>
        <script src="{{ asset ('assetsIndex/js/vendors/tooltip.js') }}"></script>
	</body>
</html>